FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      shrRate;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wallIn
    {
        type            calculated;
        value           uniform 0;
    }
    wallOut
    {
        type            calculated;
        value           uniform 0;
    }
    wallDown
    {
        type            empty;
    }
    wallUp
    {
        type            empty;
    }
    frontAndBack
    {
        type            empty;
    }
}


