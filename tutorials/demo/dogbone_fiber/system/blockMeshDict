FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

xmin -0.0054;
xmax 0.151;
ymin -0.0023;
ymax 0.0041;
zmin -0.012;
zmax 0.0131;
nx 220;
ny 10;
nz 30;

vertices
(

    ($xmin $ymin $zmin  )
    ($xmax $ymin $zmin  )
    ($xmax  $ymax $zmin )
    ($xmin  $ymax $zmin )

    ($xmin $ymin  $zmax)
    ($xmax $ymin  $zmax)
    ($xmax  $ymax  $zmax)
    ($xmin  $ymax  $zmax)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($nx $ny $nz) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
);

