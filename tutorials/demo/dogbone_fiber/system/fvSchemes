FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         cellMDLimited Gauss linear 0.5; //Gauss linear
}

divSchemes
{
    div(phi,alpha)  Gauss upwind;//Gauss vanLeer
    div(phirb,alpha) Gauss upwind;//Gauss linear

    div(rhoPhi,U)  Gauss upwind;
    div(phi,thermo:rho.poly) Gauss upwind;
    div(phi,thermo:rho.air) Gauss upwind;
    div(rhoPhi,T)  Gauss upwind;
    div(rhoPhi,K)  Gauss upwind;
    div(phi,p)      Gauss upwind;
    div(phi,k)      Gauss upwind;

    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;

    div(phi,elSigDev) Gauss upwind; //kristjan
    div(elSigDev) Gauss linear; //kristjan
    div(phi,A2) Gauss upwind; //kristjan
    div(StressFiber) Gauss linear;//NES - Kerstin

}

laplacianSchemes
{
    default         Gauss linear limited 0.777; //Gauss linear uncorrected
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         limited 0.777;
}

// ************************************************************************* //
